#include <iostream>
using namespace std;

int main() {
    int n;
    double S = 0;   // Initialize the sum
    cout << "Give n = ";
    cin >> n;

    double A[n];    // Declare the array to hold n elements
    
    // Input array elements
    for (int i = 0; i < n; i++) {
        cout << "Give A[" << i << "] = ";
        cin >> A[i];
        
        // Add to sum if the element is positive
        if (A[i] > 0) {
            S += A[i];
        }
    }
    
    // Output the final sum after all elements have been processed
    cout << "The sum of positive numbers is S = " << S << endl;

    return 0;
}
